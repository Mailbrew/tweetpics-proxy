{
  "github": {
    "silent": true
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "https://typefully-git-cooldown-image-editor-mailbrew.vercel.app/tweetpics/"
    },
    {
      "source": "/:match*",
      "destination": "https://typefully-git-cooldown-image-editor-mailbrew.vercel.app/tweetpics/:match*"
    }
  ]
}
